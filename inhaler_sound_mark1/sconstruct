import cuppa

cuppa.run(
    default_options = {
        'boost-version'             : "latest",
        'quince-location'           : "git+https://github.com/mshepanski/quince",
        'quince-postgresql-location': "git+https://github.com/mshepanski/quince_postgresql",
    },
    default_dependencies = [
        'boost',
        'quince',
        'quince-postgresql',
        'qt5'
    ]
)
